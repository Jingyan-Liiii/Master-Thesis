FLAGS		+=	-DROUNDING_FP  # if ROUNDING_FP does not work, try ROUNDING_FE or without
OFLAGS		+=	-O -g
CFLAGS		+=	$(GCCWARN)
CXXFLAGS	+=	$(GXXWARN)
ARFLAGS		=	crs
LDFLAGS		+=	-lcurses
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline
