CXX		=	g++
FLAGS           +=      -DNDEBUG 
OFLAGS          +=      -O3 -fomit-frame-pointer 
CFLAGS          +=      $(GCCWARN) -Wno-strict-aliasing -Wno-missing-declarations -Wno-missing-prototypes
CXXFLAGS        +=      $(GXXWARN) -Wno-strict-aliasing
LDFLAGS		=	-lm #-static 
ARFLAGS		=	crs
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline











